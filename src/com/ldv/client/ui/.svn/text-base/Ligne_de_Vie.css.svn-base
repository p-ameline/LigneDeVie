  margin-left: 5px;
  font-weight: normal;
  color:       #3D1429;
  text-align:  left;
}

.registerRegisterButton {
  height: 3em;
  width:  20em;
  margin-left: 20em;
}

.registerCancelButton {
  height: 2em;
  width:  10em;
  margin-left: 10px;
}

/** Validation page */

.validationMainPanel {
  height:           100%;
  width:            100%;
  border:           2px solid #777777;
  background-color: #DDFFDD;
}

.validationPagePanel {
  margin:   auto; 
  position: absolute; 
  top:      0px; 
  bottom:   0px; 
  left:     0px; 
  right:    0px; 
}

.validationMainLabel {
  font-size:   1.5em;
  font-weight: normal;
  color:       #3D1429;
  text-align:  center;
}

.validationSubLabel {
  font-size:   1em;
  font-weight: bold;
  color:       #3D1429;
  text-align:  center;
}

/** Set ids using widget.getElement().setId("idOfElement") */
#closeButton {
  margin: 15px 6px 6px;
}

.gwt-TabLayoutPanelTab .gwt-Label {
  color: black;
  font-weight: bold;
}

.gwt-TabLayoutPanelTabs .gwt-TabLayoutPanelTab {
  margin-left: 4px;
  padding: 6px 10px;
  cursor: pointer;
  cursor: hand;
  color: black;
  font-weight: bold;
  text-align: center;
  background: #e3e9ff;
}

.gwt-TabLayoutPanelTabs .gwt-Tab {
  border-left: 1px solid #999;
  border-right: 1px solid #999;
  border-top: 1px solid #999;
  float: left;
  margin: 0 4px;
  padding: 2px 8px;
  cursor: pointer;
}

.gwt-TabLayoutPanelTabs .gwt-Tab-selected {
  background-color: #bcf;
  cursor: default;
}

.gwt-TabLayoutPanelTabs .gwt-TabLayoutPanelTab-disabled {
  cursor: default;
  color: #999999;
}

.gwt-TabLayoutPanelTabs .gwt-TabInner {
  color: black;
  font-weight: bold;
  text-align: center;
}

/* Splitters */
.gwt-SplitLayoutPanel-HDragger {
  cursor: col-resize;
}

.gwt-SplitLayoutPanel-VDragger {
  cursor: row-resize;
}

