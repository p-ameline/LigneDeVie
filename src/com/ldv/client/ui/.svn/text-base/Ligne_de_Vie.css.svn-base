}
.ldv-TimeControl-leftScrollButton {
  left: 0px;
  top: 0px;
  width: 25px;
  height: 100%;
  overflow: hidden;
}
.ldv-TimeControl-rightScrollButton {
  right: 0px;
  top: 0px;
  width: 25px;
  height: 100%;
  overflow: hidden;
}
.ldv-TimeControl-scrollThumb {
  left: 100px;
  top: 0px;
  width: 30px;
  height: 100%;
  background: blue;
  z-index: 1;
  cursor: pointer;
}

