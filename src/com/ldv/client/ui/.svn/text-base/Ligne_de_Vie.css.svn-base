/** Example rules used by the template application (remove for your app) */
h1 {
  font-size: 2em;
  font-weight: bold;
  color: #777777;
  margin: 5px 0px 5px;
  text-align: center;
}

h2 {
  font-size: 1em;
  font-weight: bold;
  color: #FF7777;
  margin: 5px 0px 5px;
  text-align: left;
}

.ldvMainPanel {
  background-color: #FFFFFF;
  display:  block;
  float:    left;
  width:    100%;
  height:   100%; 
}

.sendButton {
  display: block;
  font-size: 16pt;
}

/** Most GWT widgets already have a style name defined */
.gwt-DialogBox {
  width: 400px;
}

.dialogVPanel {
  margin: 5px;
}

.loginVPanel {
  margin: 5px;
  padding: 10px;
  border: 2px solid #999;
}

.serverResponseLabelError {
  color: red;
}

/** Header block */

.ldvHeaderPanel {
  background-color: #2E0F1F;
  display:  block;
  float:    left;
  width:    100%;
  height:   3.5em; 
  border-bottom: 1px solid #993366;
}

.ldvHeaderTitle {
  font-size: 2em;
  font-weight: bold;
  color: #FFFFFF;
  margin: 5px 0px 5px;
}

/** Body block */

.workspace {
  background-color: #FFFFFF;
  display:  block;
  float:    left;
  width:    100%;
  height:   25em; 
}

.ldvBodyPanel {
  background-color: #FFFFFF;
  display:  block;
  float:    left;
  width:    100%;
  height:   100%;
}

.ldvVideoArea {
  background-color: #FFFFFF;
  display:  inline-block;
  float:    left;
  width:    49%;
  height:   100%; 
}

.bodyRightPanel {
  background-color: #FFFFFF;
  display:  inline-block;
  float:    left;
  width:    50%;
  height:   100%; 
}

/** Footer block */

.ldvFooterPanel {
  background-color: #FFFFFF;
  display:  block;
  float:    left;
  width:    100%;
  height:   2em; 
}

/** Login block */

.panelTable {
  height:           5em;
  border:           2px solid #777777;
  background-color: #EBD6E0;
}

.loginTable {
	display:  block;
  height:           auto;
  width:            80%;
  margin:           5px;
  padding:          5px;
  border:           2px solid #F5EBF0;
  \-webkit-border-radius: 1em; 
  \-moz-border-radius: 1em;
  border-radius: 1em;
    /* fallback */ 
  background: #EBD6E0; 
  /* background: url(images/linear_bg_2.png); */ 
  /* background-repeat: repeat-x; */ 
  /* Firefox 3.6+ */ 
  /* background: -moz-linear-gradient(100% 100% 90deg, #E0C2D1, #F5EBF0); */
  background: \-moz-linear-gradient(top,  #E0C2D1,  #F5EBF0); 
  /* Safari 4-5, Chrome 1-9 */ 
  /* -webkit-gradient(<type>, <point> [, <radius>]?, <point> [, <radius>]? [, <stop>]*) */ 
  background: \-webkit-gradient(linear, 0% 0%, 0% 100%, from(#F5EBF0), to(#E0C2D1)); 
  /* Safari 5.1+, Chrome 10+ */ 
  background: \-webkit-linear-gradient(#E0C2D1, #F5EBF0); 
  /* Opera 11.10+ */ 
  background: \-o-linear-gradient(#E0C2D1, #F5EBF0);
}

.loginIdLabel {
	display:     inline-block;
	float:       left;
  height:      1.5em;
  width:       8em;
  margin-top:  10px;
  margin-left: 10px;
  font-size:   1.5em;
  font-weight: normal;
  color:       #777777;
  text-align:  left;
}

.loginIdText {
	display:     block;
	width:       15em;
	margin-top:  10px;
  font-size:   1.5em;
  font-weight: normal;
  color:       #000000;
  text-align:  left;
}

.loginPassLabel {
	display:     inline-block;
  float:       left;
  height:      1.5em;
  width:       8em;
  margin-left: 10px;
  font-size:   1.5em;
  font-weight: normal;
  color:       #777777;
  text-align:  left;
}

.loginPassText {
	display:     block;
	width:       15em;
  font-size:   1.5em;
  font-weight: normal;
  color:       #000000;
  text-align:  left;
}

.loginButton {
	margin-top: 10px;
  height: 3em;
  width:  50%;
  margin-left:  25%;
}

/** Register invite block */

.registerPanel {
  display:  block;
  height:           auto;
  width:            80%;
  margin:           5px;
  margin-top:      25px;
  padding:          5px;
}

.registerLabel {
  font-size:   1.5em;
  font-weight: normal;
  color:       #3D1429;
  text-align:  center;
}

.registerButton {
  margin-top: 10px;
  height: 3em;
  width:  50%;
  margin-left:  25%;
}

/** Register page */

.panelTable {
  height:           100%;
  width:            100%;
  border:           2px solid #777777;
  background-color: #DDFFDD;
}

.registerMainPanel {
  display:     block;
  height:      auto;
  width:       80%;
  margin:      5px;
  margin-left: 10%;
  padding:     5px;
}

.registerPageBirthdateTable {
  margin:           5px;
  padding:          5px;
}

.registerPageLine {
	display:     block;
  float:       left;
  width:       100%;
  margin-left: 5px;
  font-weight: normal;
  color:       #3D1429;
  text-align:  left;
}

.registerPageSeparLine {
  display:     block;
  float:       left;
  width:       100%;
  margin-top:  5px;
  margin-left: 5px;
  font-weight: normal;
  color:       #3D1429;
  text-align:  left;
}

.registerPageLabel {
	display:     block;
  float:       left;
	width:       20em;
  font-size:   1.1em;
  font-weight: normal;
  color:       #3D1429;
  text-align:  left;
}

.registerPageTextBox {
	display:     block;
  float:       left;
	width:       25em;
  font-size:   1.1em;
  font-weight: normal;
  color:       #3D1429;
  text-align:  left;
}

.registerPageError {
  font-size:   1em;
  font-weight: bold;
  color:       #DD0000;
  text-align:  left;
  margin-left: 5px;
}

.registerPageSeparButtons {
  display:     block;
  float:       left;
  width:       100%;
