package com.ldv.client.ui;

/**
 * An object that implements this interface may be enabled or disabled.
 * 
 * @author lowec
 * 
 */
public interface HasEnable {

	/**
	 * Is the object enabled?
	 * 
	 * @return true if enabled, false otherwise
	 */
	boolean isEnabled();

	/**
	 * Set the object's enabled state.
	 * 
	 * @param enabled
	 *            true if enabled, false otherwise
	 */
	void setEnabled(boolean enabled);
	
}

