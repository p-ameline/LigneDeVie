package com.ldv.client;

import com.ldv.shared.model.LdvTime;

public class LdvConcernSeverity
{
	protected int     _iSeverityLevel ;
	protected LdvTime _tSeverityDate ;
	
	public LdvConcernSeverity()
	{
		init() ; 
	}
	
	void init()
	{
		_iSeverityLevel = 0 ;
		_tSeverityDate.init() ;
	}

	public int get_iSeverityLevel()
  {
  	return _iSeverityLevel;
  }

	public void set_iSeverityLevel(int iSeverityLevel)
  {
  	_iSeverityLevel = iSeverityLevel;
  }

	public LdvTime get_tSeverityDate()
  {
  	return _tSeverityDate;
  }

	public void set_tSeverityDate(LdvTime tSeverityDate)
  {
  	_tSeverityDate = tSeverityDate;
  }
}
