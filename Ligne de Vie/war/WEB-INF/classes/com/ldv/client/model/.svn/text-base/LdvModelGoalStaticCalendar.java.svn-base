package com.ldv.client.model;

public class LdvModelGoalStaticCalendar
{
	protected String _sMinAllowedStartDate ; // minimal allowed start date: from red to yellow
	protected String _sMinGoodStartDate ;    // minimal good start date: from yellow to green
	protected String _sMinIdealStartDate ;   // minimal ideal start date: from green to blue
	protected String _sMaxIdealStartDate ;   // maximal ideal start date: from blue to green
	protected String _sMaxGoodStartDate ;    // maximal good start date: from green to yellow 
	protected String _sMaxAllowedStartDate ; // maximal allowed start date: from yellow to red
	
	public LdvModelGoalStaticCalendar()
	{
		init() ; 
	}
	
	void init()
	{
		_sMinAllowedStartDate = "" ;
		_sMinGoodStartDate    = "" ;
	  _sMinIdealStartDate   = "" ;
		_sMaxIdealStartDate   = "" ;
		_sMaxGoodStartDate    = "" ;
		_sMaxAllowedStartDate = "" ;
	}
}
