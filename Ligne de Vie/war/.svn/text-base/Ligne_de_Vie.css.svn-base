.ldv-context-label {
  background-color: #ffffff;
  font-size: 8pt;
  text-align: center;
}
.ldv-TimeControl-contextsArea {
  border: 1px solid black;
  /*border-right: 2px solid #848280;*/
  /* border-bottom: 2px solid #848280;*/
  background-color: #4169e1;
  height: 25%;
  width: 100%;
}
.ldv-TimeControl-scrollBar {
  border: 1px solid black;
  background-color: white;
  height: 25%;
  width: 100%;
  /*top: 22pt;*/
  overflow: hidden;
}
.ldv-TimeControl-leftScrollButton {
  left: 0px;
  top: 0px;
  width: 25px;
  height: 100%;
  overflow: hidden;
}
.ldv-TimeControl-rightScrollButton {
  right: 0px;
  top: 0px;
  width: 25px;
  height: 100%;
  overflow: hidden;
}
.ldv-TimeControl-scrollThumb {
  left: 100px;
  top: 0px;
  width: 30px;
  height: 100%;
  background: blue;
  z-index: 1;
  cursor: pointer;
}

