package com.ldv.shared.graph;

public class LdvModelRight
{
	protected String _sNODE ;
	protected String _sRIGHT ;
	
	public LdvModelRight()
	{
		init() ; 
	}
	
	public LdvModelRight(String sNode, String sRight)
	{
		_sNODE  = sNode ;
		_sRIGHT = sRight ;
	}
	
	void init()
	{
		_sNODE  = "" ;
		_sRIGHT = "" ;
	}
	
	public String getNode()
  {
  	return _sNODE ;
  }
	public void setNode(String sNode)
  {
		_sNODE = sNode ;
  }

	public String getRight()
  {
  	return _sRIGHT ;
  }
	public void setQualifier(String sRight)
  {
		_sRIGHT = sRight ;
  }
}
